@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

@{
    ViewData["Title"] = "Список ролей";
}

<h2 class="mt-4">Список ролей</h2>

<table class="table">
    <thead>
        <tr>
            <th>Назва ролі</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="UserList" class="btn btn-primary">Список користувачів</a>